<div class="showcase-index-wrapper">
  <div class="container">
    <p><%= @nickname %>さんのページ</p>
    <%= @showcases.each do |showcase| %>
      <div class="row">
        <div class="col-md-6 col-md-offset-3">
          <div class="showcase-index-single">
            <%= link_to mypage_showcases_path(showcase_id: :showcase_id) %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
